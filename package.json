    "@storybook/addon-ondevice-controls": "^8.4.2",
    "@storybook/react-native": "^8.4.2",
    "@types/jest": "^29.5.12",
    "@types/react": "~18.3.12",
    "@types/react-test-renderer": "^18.3.0",
    "babel-loader": "^8.4.1",
    "cross-env": "^7.0.3",
    "jest": "^29.2.1",
    "jest-expo": "~52.0.2",
    "react-native-svg": "^15.9.0",
    "react-test-renderer": "18.3.1",
